<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.pharmaClick" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="FacebookConnectPlugin">
        <param name="android-package" value="org.apache.cordova.facebook.ConnectPlugin" />
        <param name="onload" value="true" />
    </feature>
    <access origin="https://m.facebook.com" />
    <access origin="https://graph.facebook.com" />
    <access origin="https://api.facebook.com" />
    <access origin="https://*.fbcdn.net" />
    <access origin="https://*.akamaihd.net" />
    <feature name="GooglePlus">
        <param name="android-package" value="nl.xservices.plugins.GooglePlus" />
        <param name="onload" value="true" />
    </feature>
    <access origin="https://accounts.google.com/*" />
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <feature name="CallNumber">
        <param name="android-package" value="mx.ferreyra.callnumber.CFCallNumber" />
    </feature>
    <feature name="SpinnerDialog">
        <param name="android-package" value="hu.dpal.phonegap.plugins.SpinnerDialog" />
    </feature>
    <name>PharmaClick</name>
    <description>Pharma Click App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Varahi Technologies Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <icon density="ldpi" src="src/assets/images/PharamClick04.png" />
    <icon density="mdpi" src="src/assets/images/PharamClick04.png" />
    <icon density="hdpi" src="src/assets/images/PharamClick04.png" />
    <icon density="xhdpi" src="src/assets/images/PharamClick04.png" />
    <icon density="xxhdpi" src="src/assets/images/PharamClick04.png" />
    <icon density="xxxhdpi" src="src/assets/images/PharamClick04.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="android-targetSdkVersion" value="32" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="backgroundColor" value="#FFFFFF" />
    <preference name="AndroidWindowSplashScreenAnimatedIcon" value="src/assets/images/PharmaClick1.png" />
</widget>
